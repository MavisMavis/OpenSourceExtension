{
  "manifest_version": 2,
  "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
  },
  "name": "SSearch",
  "description": "Search function for searching music, videos, eBooks and other content throughout the web.",
  "version": "1.0",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      }
    }
  },
  
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "popout/popout.html"
  }
}
